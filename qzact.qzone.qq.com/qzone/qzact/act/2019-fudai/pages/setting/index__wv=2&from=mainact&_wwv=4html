            var android = ua.match(/Android/),
                iPad = ua.match(/(iPad).*OS\s([\d_]+)/),
                iPhone = !iPad && (ua.match(/(iPhone\sOS)\s([\d_]+)/i) || ua.match('iPhone')),
                iPod = ua.match(/(iPod).*OS\s([\d_]+)/);
            window.browserInfo = {
                isQQ: ua.match(/QQ\/([\d\.]+)/),
                isIos: iPad || iPhone || iPod,
                isMobile: android || iPhone
            };
            if (window.mqq) {
                window.reportFudai = function (params) {
                    if (window.mqq) {
                        console.info('report fudai data:', params);
                        mqq.invoke('FD', 'ReportData', {data: params});
                    }
                };
                window.reportFudai({act_id: '92'});
                mqq.ui.setWebViewBehavior({
                    progressBar: false,
                    titleText: '',
                    actionButton: 0
                });
                mqq.invoke('ui', 'webviewCanScroll', {"enable": false});
            }
        })()
