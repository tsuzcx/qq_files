MOOD.tmp.hotTopic = '<%for(var i=0; i<items.length; ++i){var item = items[i];%><li class="bg2"><p class="itm bor3"><span class="c_tx3" id="<%=item.likekey%>"><%=item.count%></span><a href="http://rc.qzone.qq.com/qzonesoso/?search=<%=encodeURIComponent(escHTML(item.keyword))%>" class="c_tx up goSearch" keyword="<%=item.keyword%>" target="_blank" onclick="return false;" title="<%=escHTML(item.title)%>"><%=escHTML(item.title)%></a></p><%if(item.tip){%><p class="con bg bor3"><span class="arr"><i></i></span><%=escHTML(item.tip)%></p><%}%></li><%}%>';
