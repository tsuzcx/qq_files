<script type="text/javascript">
var elements = document.getElementsByTagName('a');

for(var i=0;i<elements.length;i++) {
	var ele = elements[i];
    if(location.href.indexOf('g_iframeUser=1') > -1 && ele.href.indexOf('user.qzone.qq.com') > -1) {
    	var _href = ele.href.indexOf('?')>-1?(ele.href+'&g_iframeUser=1'):(ele.href+'?g_iframeUser=1');
    	ele.href = _href;
    }
}

window.QZONE = window.QZONE || {};

// global
var isQzone = document.domain == "qq.com" || document.domain == "user.qzone.qq.com";
var isOwner = MOOD.env.uin == MOOD.env.loginUin;
(function(QZONE){
	/*将来可以移除*/
	QZONE.FP = QZONE.FP || parent.QZONE.FrontPage;
	QZONE.PY = QZONE.PY || parent.QZONE.PengYou;
	var isLogin = 0!= QZONE.FP.getQzoneConfig("loginUin");
	/*将来可以移除*/

	var isLite = (function(){
		if (QZONE.PY && QZONE.PY.hasLeftBar){
			return QZONE.PY.hasLeftBar();
		}else{
			return false;
		}
	})();
	document.write([
	'<style>',
		(isLite?'.lite .bg_mode .box_ml { border-style: none;} .lite .bg_mode .box_ml .app_mood .container .main{padding-top: 0;} .lite .bg_mode .box_ml .app_mood .container .main .feed_wrap{ margin-top: 0;}':''),
		'.lite_hide {', (isLite ? 'display:none':''),'}',
		(isLite ? '.lite .app_mood .container .main{ margin-right: 0; }':''),
		'.qzone_display {',(isQzone ? '' : 'display:none !important;'),'}',
		'.pengyou_display {',(isQzone ? 'display:none !important;' : ''),'}',
		'.author_display {',(isOwner ? '' : 'display:none !important;'),'}',
		'.visitor_display {',(isOwner ? 'display:none !important;' : ''),'}',
		'.login_display {',(isLogin ? '' : 'display:none !important;'),'}', 
	'</style>'
	].join(''));
})(QZONE);
MOOD.tmp = MOOD.tmp || {};
