<script  type="text/javascript">
var MOOD = MOOD || {};

(function(MOOD){
	MOOD.version = '20170728';
	MOOD.cssVersion = '20170728';
	MOOD.env = MOOD.env || {};
	MOOD.cookie = MOOD.cookie || {};

	MOOD.cookie.get = function(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return '';
	}

	MOOD.env.getCSRFToken = function(){
		var str = MOOD.cookie.get('skey');
		var hash = 5381;
		for(var i = 0, len = str.length; i < len; ++i){
			hash += (hash << 5) + str.charAt(i).charCodeAt();
		}
		return hash & 0x7fffffff;
	};

	MOOD.env.getLoginUin = function(){
		var uin = parseInt(MOOD.cookie.get('uin').replace(/[^\d]/g, ''), 10);
		if(uin){
			return uin + '';
		}
		return top.g_iLoginUin + '';
	};

	MOOD.util = MOOD.uitl || {};

	MOOD.util.unixTimeToString = function(config){
		config = config || {};
		config.type = config.type || 0;
		timestamp = (config.timestamp || 0) * 1000;
		var now;
		if (timestamp>0){
			now = new Date(timestamp);
		}else{
			now = new Date();
		}

		var month = now.getMonth() + 1;
		var date = now.getDate();
		var hours = now.getHours();
		var minutes = now.getMinutes();
		var seconds = now.getSeconds();

		var datetime = {
			'YYYY': now.getFullYear(),
			'mm': month>9?month:('0'+month),
			'dd': date>9?date:('0'+date),
			'H': hours>9?hours:('0'+hours),
			'i': minutes>9?minutes:('0'+minutes),
			's': seconds>9?seconds:('0'+seconds)
		};

		var date_str = datetime['YYYY'] + '-' + datetime['mm'] + '-' + datetime['dd'] + ' ';
		var time_str = datetime['H'] + ':' + datetime['i'] + ':' + datetime['s'];

		var result = '';
		switch(config.type){
			case 1:
				result = date_str;
				break;
			case 2:
				result = time_str;
				break;
			default:
				result = (date_str + ' ' + time_str)
		}
		return result;
	};

	MOOD.util.getHttpParams = function(key){
		var r = new RegExp('(\\?|#|&)'+key+'=([^&#]*)(&|#|$)');
		var m = location.href.match(r);
		var v = decodeURIComponent(!m?'':m[2]).replace(/\+/g,' ');
		return OOP.tpl.filter(v);
	}

	MOOD.lang = MOOD.lang || {};

	/**
	 * @static
	 * @method randomArray
	 * @param {Array} target The array to random.
	 * @return {Array}
	 */
	MOOD.lang.randomArray = function RandomArray(target) {
		if (Object.prototype.toString.apply(target) != '[object Array]') {
			return target;
		}
		var r, tmp, len = target.length;
		for (var i = 0; i < len; i++) {
			r = Math.floor(Math.random() * len);
			tmp = target[r];
			target[r] = target[i];
			target[i] = tmp;
		}
		return target;
	};
})(MOOD);

/* simple string format */
var OOP = OOP || {};
(function(OOP){
	OOP.tpl = OOP.tpl || {};
	var getByPath = function (obj,path,parma){
		obj = obj || {};
		var paths = path.split('.'), len, i;
		len = paths.length;
		for(i=0;i<len;i++){
			obj =  obj[paths[i]];
			if(typeof obj == 'function'){
				obj = obj();
			}
			if (obj === undefined || obj === null) {
				break;
			}
		}
		return obj;
	}

	OOP.tpl.format = function(tpl,obj){
		var formatRegex = /\{{([\d\w\.]+)\}}/g,v;
		return tpl.replace(formatRegex,function(m,n){
			v = getByPath(obj,n);
			return v === undefined ?'':v;
		});
	}

	OOP.tpl.html = function (string) {
		var HTML_CHARS = {
	        '&': '&amp;',
	        '<': '&lt;',
	        '>': '&gt;',
	        '"': '&quot;',
	        "'": '&#x27;',
	        '/': '&#x2F;',
	        '`': '&#x60;'
		};
	    var r = (string + '').replace(/[&<>"'\/`]/g, function(match){
	    	return HTML_CHARS[match];
	    });
	    return r;
	};
	OOP.tpl.filter = function (string) {
	    var r = (string + '').replace(/[&<>"'\/`]/g, function(match){
	    	return '';
	    });
	    return r;
	};
})(OOP);
/* simple string format end */




(function(){

	var realUin = top.QZONE && top.QZONE.FP && top.QZONE.FP.getQzoneConfig && (top.QZONE.FP.getQzoneConfig('loginUin')>0 || (top.QZONE.FP.getQzoneConfig('loginUin')+'').length >4)  && top.QZONE.FP.getQzoneConfig('loginUin');
	var loginUin = realUin || MOOD.env.getLoginUin();
	var queryUin = MOOD.util.getHttpParams('uin') || loginUin;

	var isOwner = loginUin == queryUin;
	//统一都直接进我的说说
	var tab = MOOD.util.getHttpParams('tab') || 'mine';//(isOwner? 'mine':'mine');
	var params = OOP.tpl.filter(MOOD.util.getHttpParams('params')).split(',');
	var action = MOOD.util.getHttpParams('action');
	//var actionParams = MOOD.util.getHttpParams('poster');
	var tid = MOOD.util.getHttpParams('tid') , t1_source = MOOD.util.getHttpParams('source');
	var url = '';
	var prefix = window.location.protocol === 'https:' ? ('https://'+(top.g_cgidomain||'h5')+'.qzone.qq.com/proxy/domain/') : 'http://';
	var tokenDomain = window.location.protocol === 'https:' ? 'http://taotao.qzone.qq.com' :  'http://taotao.qq.com'; //https下，走h5代理，所以token要用qzone.qq.com来算
	
	//启动逻辑，区分参数，进入不同的页面
	if(tab=='timer'){
		tab = 'schedule';
	}
	switch(params[0]){
		case 'mymood' : //我的说说 QZONE.FP.toApp('/mood/mymood/timer'); 去定时说说接口
			tab = 'mine';
			if(params[1] == 'timer'||params[1] == 'schedule'){
				tab = 'schedule';
			}
			break;
		case 'list':
			tab = 'mine';
			queryUin = params[1] || loginUin;
			isOwner = loginUin == queryUin;
			break;
		case 'mine':
			tab = 'mine';
			break;
		case 'setting' : //设置页面
			url = 'https://user.qzone.qq.com/proxy/domain/qzs.qq.com/qzone/app/mood_v6/html/setting.html';
			window.location = url;
			break;
		case 'jumpToAudit' : //评论审核
			url = OOP.tpl.format(prefix + 'taotao.qq.com/cgi-bin/emotion_cgi_censor_get?uin={{uin}}&pos=0&num=10&g_tk={{token}}',{uin:queryUin,token:QZONE.FP.getACSRFToken(tokenDomain)});
			window.location.href = url;
			break;
		case 'audit' : //审核
			url = OOP.tpl.format(prefix + 'taotao.qq.com/cgi-bin/emotion_cgi_censor_get?uin={{uin}}&pos=0&num=10&g_tk={{token}}',{uin:queryUin,token:QZONE.FP.getACSRFToken(tokenDomain)});
			window.location.href = url;
			break;
		case 'timer':
		case 'schedule' : //定时说说
			tab = 'schedule';
			break;
		default :
			if(/[0-9a-z]+/i.test(params[0])){//详情
				var p = params[0].split('.');
				tid = p[0];
				t1_source = p[1] || '1';
				tab = 'detail';
			}
			if(tid && t1_source){
				tab = 'detail';
			}
	}
	if(params[0]=='audit' || params[0]=='jumpToAudit'){ //如果params是这两个，就不要往下执行了

		return;
	}
	MOOD.env = MOOD.env || {};
	MOOD.env.t1_source = t1_source;
	MOOD.env.tid = tid;
	MOOD.env.uin = queryUin;
	MOOD.env.loginUin = loginUin;
	MOOD.env.tab = tab;
	MOOD.env.action = action;



	window._preloadCache = {
		code: 9999,
		message: '很抱歉，当前网络繁忙，请稍候再试！',
		result: {code:9999,msg:'很抱歉，当前网络繁忙，请稍候再试！'}
	};

	if(tab=='hot'){
		/**hack for 热门说说**/
		window._Callback = function(d){
			window._Callback = null;
			window._LocalCacheCallbackData = d;
			var msglist = [];
			var data  = window._LocalCacheCallbackData || {};
			data.data = data.data || [];
			for(var i =0;i<data.data.length&&i<20;i++){
				msglist.push(window._fakeData(data.data[i]));
			}
			MOOD.lang.randomArray(msglist);
			window._preloadCache.auth_flag = 1;
			window._preloadCache.msglist = msglist;
			window._preloadCache.total = data.totalCnt;//data.data.length;
			gTimeStampsPool[5]=(new Date());
		};
	}

	window._LocalCache = function(d){
		window._LocalCache = null;
		window._preloadCache = d;
	};

	window._fakePic = function(image){
		var data = {
			height : image.length,
			pictype : 0,
			richsubtype : 2,
			url1 : image.MPicUrl,
			url2 : image.BPicUrl,
			url3 : image.BPicUrl,
			width : image.width
		};
		return data;
	}

	window._fakeData = function(item){
		var data = {
			auth_flag: 1,
			certified : 1,
			conlist : [],
			content : item.text,
			created_time: 0,
			createTime : MOOD.util.unixTimeToString({timestamp:item.created}),
			dest_name : '',
			//em_alt : "一般",
			//em_head : -1,
			// lbs :{
			// 	id : '',
			// 	idname : '',
			// 	name : '',
			// 	pos_x : '',
			// 	pos_y : ''
			// },
			name : item.nickname,
			//ppos : 0,
			//puin : 721289260,
			replies : 0,
			right : 1,
			rt_sum : 0,
			source_name : '',
			source_url : '',
			t1_source : 1,
			t1_subtype : 1,
			t1_termtype : 0,
			tid : item.source.tid,
			uin : item.source.uin
			//urltype : 1
		};
		var pics = [];
		item.images= item.images || []
		for(var i = 0;i<item.images.length;i++){
			pics.push(window._fakePic(item.images[i]));
		}

		if(pics.length>0){
			data.pic = pics;
			data.pictotal =  pics.length;
		}

		var videos = [];
		item.videos = item.videos || [];
		for(var i = 0;i<item.videos.length;i++){
			videos.push({
			url1: item.videos[i].vimagUrl,
			url2: item.videos[i].SrcUrl,
			url3: item.videos[i].PlayerUrl
			});
			/*@deprecated
			data.url1 = item.videos[i].vimagUrl;
			data.url2 = item.videos[i].SrcUrl;
			data.url3 = item.videos[i].PlayerUrl;
			data.urltype = 3;*/
		}

		if(videos.length>0){
			data.video = videos;
			data.videototal  = videos.length;
		}
		return data;
	}
	var _protocol = 'https:';
	if(top.location.href.indexOf('forcehttp') > -1){
		_protocol = 'http:';
	}
	/**hack for 热门说说结束**/
	var moodAppCss = (qzoneVersion==6 ? [
		'<link rel="stylesheet" rev="stylesheet" href="//{{siDomain}}/qzone_v6/gb/skin/{{g_StyleID}}.css" type="text/css" media="screen" \/>',
		'<link rel="stylesheet" rev="stylesheet" href="//{{siDomain}}/qzone_v6/say.css?max_age=999999&v='+MOOD.cssVersion+'" type="text/css" media="screen" \/>',
		'<link rel="stylesheet" rev="stylesheet" href="//{{siDomain}}/qzone_v6/qz_shuoshuo_audio.css?max_age=999999&v='+MOOD.cssVersion+'" type="text/css" media="screen" \/>'
		] :
	 [
		'<link rel="stylesheet" rev="stylesheet" href="'+ _protocol +'//{{siDomain}}/aoi/skin/{{g_StyleID}}.css" type="text/css" media="screen" \/>',
		'<link rel="stylesheet" rev="stylesheet" href="//{{siDomain}}/aoi/say.css?max_age=999999&v='+MOOD.cssVersion+'"" type="text/css" media="screen" \/>',
		'<link rel="stylesheet" rev="stylesheet" href="//{{siDomain}}/qzone_v6/qz_shuoshuo_audio.css?max_age=999999&v='+MOOD.cssVersion+'"" type="text/css" media="screen" \/>'
	]);
	var _load = moodAppCss.concat([

		'<script type="text/javascript" src="{{proto}}//{{siDomain}}/ac/qzone/qzfl/qzfl{{qz}}.js"><\/script>',
		'<script type="text/javascript" src="{{proto}}//{{siDomain}}/qzone/app/mood_v6/api/api.js?max_age=999999&v='+MOOD.version+'"><\/script>',
		'<script type="text/javascript" src="{{proto}}//{{siDomain}}/qzone/app/mood_v6/script/render.js?max_age=999999&v='+MOOD.version+'"><\/script>',
		top == window ? '<script type="text/javascript" src="//{{siDomain}}/qzone/v6/interface/interface_v6{{g_V.ci}}.js?max_age=2592000"><\/script>':''
		
		]).join('');
	var location = '';
	//idcNo为2表示用上海的cgi
	if(window._locationCache && window._locationCache.idcNo==2){
		location = 'sh.';
	}
	qzlog.info({cm:"MOOD_APP",cf:"LOAD_DATA_START"});
	timeStampsPool[1]=(new Date());
	var _data = {
		imgcacheDomain : imgcacheDomain,
		siDomain:siDomain,
        proto : window.g_cdn_proto || '',
		g_StyleID:g_StyleID,
		g_V:g_V,
		qz : g_V.qz,
		sea : g_V.sea || '2.0.0',
		host: document.domain,
		uin: queryUin,
		random: Math.floor(Math.random()*25),
		token: MOOD.env.getCSRFToken(),
		location : location
		
	};
	if (location.search != ''){
		document.write(OOP.tpl.format(_load,_data));
	}
	gTimeStampsPool[4]=(new Date());
})();
</script>
